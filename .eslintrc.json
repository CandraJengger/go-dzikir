{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "prettier"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react", "prettier"
    ],
    "rules": {
        "prettier/prettier": "error",
        "react/jsx-props-no-spreading": 0,
        "react/require-default-props":0, 
        "react/forbid-prop-types": 0,
        "import/prefer-default-export": 0
    },
    "parser": "babel-eslint",
    "ignorePatterns": ["src/reportWebVitals.js","src/serviceWorkerRegistration.js","src/setupTests.js","src/service-worker.js"]
}
